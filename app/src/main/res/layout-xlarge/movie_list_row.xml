<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="?android:attr/selectableItemBackground"
    android:clickable="true"
    android:focusable="true"
    android:orientation="horizontal"
    android:paddingBottom="10dp"
    android:paddingLeft="4dp"
    android:paddingRight="4dp"
    android:paddingTop="10dp">

    <ImageView
        android:id="@+id/airlinelogo"
        android:layout_width="80dp"
        android:layout_height="50dp" />

    <TextView
        android:id="@+id/tv_airlinecode"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/year"
        android:textSize="@dimen/report_text_size" />


    <TextView
        android:id="@+id/tv_coupen"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/year"
        android:textSize="@dimen/report_text_size" />

    <!--<TextView-->
        <!--android:id="@+id/tv_class"-->
        <!--android:layout_width="70dp"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:gravity="center"-->
        <!--android:textColor="@color/year"-->
        <!--android:textSize="@dimen/report_text_size" />-->

    <TextView
        android:id="@+id/tv_voidcxl"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/year"
        android:textSize="@dimen/report_text_size" />


    <TextView
        android:id="@+id/tv_basefare"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_yqtax"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:gravity="right"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
    android:id="@+id/tv_otherTaxAmnt"
    android:layout_width="150dp"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:textSize="@dimen/report_text_size"
    android:textColor="@color/DarkBlue"/>

    <TextView
        android:id="@+id/tv_comRec"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_orAmnt"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_incFlow"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_plbAmnt"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_acmAdm"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <!--<TextView-->
        <!--android:id="@+id/tv_gst"-->
        <!--android:layout_width="150dp"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:gravity="center"-->
        <!--android:textColor="@color/DarkBlue"-->
        <!--android:textSize="@dimen/report_text_size" />-->

    <TextView
        android:id="@+id/tv_netAmnt"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_branch"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Total"
        android:textColor="@color/year"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_airlineName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Total"
        android:textColor="@color/year"
        android:textSize="@dimen/report_text_size" />

</LinearLayout>