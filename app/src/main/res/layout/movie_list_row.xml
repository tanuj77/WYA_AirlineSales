<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:paddingLeft="4dp" android:paddingRight="4dp"
    android:paddingTop="10dp" android:paddingBottom="10dp" android:clickable="true"
    android:background="?android:attr/selectableItemBackground"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/airlinelogo"
        android:layout_width="50dp"
        android:layout_height="30dp"
        />
    <TextView android:id="@+id/tv_airlinecode"
        android:textColor="@color/year"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="@dimen/report_text_size"
        />

    <TextView
        android:id="@+id/tv_coupen"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:textColor="@color/year"
        android:gravity="center"
        android:textSize="@dimen/report_text_size"/>



    <TextView
        android:id="@+id/tv_voidcxl"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size"/>

    <TextView android:id="@+id/tv_basefare"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="@color/DarkBlue"
        android:gravity="right"
        android:textSize="@dimen/report_text_size"
        />

    <TextView android:id="@+id/tv_yqtax"
        android:textColor="@color/DarkBlue"
        android:gravity="right"
        android:layout_alignParentTop="true"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/report_text_size"
        />
    <TextView
        android:id="@+id/tv_otherTaxAmnt"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="@dimen/report_text_size"
        android:textColor="@color/DarkBlue"/>

    <TextView
        android:id="@+id/tv_comRec"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_orAmnt"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_incFlow"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_plbAmnt"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <TextView
        android:id="@+id/tv_acmAdm"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />

    <!--<TextView-->
        <!--android:id="@+id/tv_gst"-->
        <!--android:layout_width="110dp"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:gravity="center"-->
        <!--android:textColor="@color/DarkBlue"-->
        <!--android:textSize="@dimen/report_text_size" />-->

    <TextView
        android:id="@+id/tv_netAmnt"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/DarkBlue"
        android:textSize="@dimen/report_text_size" />


    <TextView android:id="@+id/tv_branch"
        android:textColor="@color/year"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Total"
        android:textSize="@dimen/report_text_size" />

    <TextView android:id="@+id/tv_airlineName"
        android:textColor="@color/year"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Total"
        android:textSize="@dimen/report_text_size" />


</LinearLayout>